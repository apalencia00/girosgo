<?xml version='1.0' encoding='utf-8'?>
<widget id="com.girosgo.domi" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DomiReady</name>
    <icon density="mdpi" height="57" platform="ios" src="www/res/ready.png" width="57" />
    <description>
        DomiReady
    </description>
    <author email="palencia89@gmail.com" href="http://cordova.io">
        Andres Palencia Florez
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="sms:*" />
    <allow-intent href="tel:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="*" />

    
    
    <access origin="https://*"/>
    <access origin="http://*"/>
    <allow-navigation href="http://*"/>
    <allow-navigation href="https://*"/>

    <access origin="*" />

      
    <access origin="http://sockjs.pusher.com" /> 
    <access origin="https://google.com" />
    <access origin="http://maps.google.com" />
    <access origin="http://*.google.com" />
    <access origin="content:///*" />
    <access origin="*" />
    <allow-navigation href="http://*"/>
    <allow-navigation href="https://*"/>
    <platform name="android">
        
        <preference name="loadUrlTimeoutValue" value="700000" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="ios" spec="^5.0.1" />
    <plugin name="cordova-plugin-whitelist" spec="1.3.4" />
    <plugin name="cordova-plugin-camera" spec="~4.0.3" />
    <plugin name="phonegap-plugin-barcodescanner" spec="~8.1.0">
        
    </plugin>
    <plugin name="phonegap-plugin-push" spec="~2.3.0">
        <variable name="SENDER_ID" value="helpdesk-fe8b4" />
        <variable name="ANDROID_SUPPORT_V13_VERSION" value="28.+" />
        <variable name="FCM_VERSION" value="17.0.+" />
    </plugin>
    <plugin name="cordova-plugin-qr-barcode-scanner" spec="~8.0.3" />

<feature name="Geolocation">
    <param name="Android-package" value="org.Apache.cordova.GeoBroker" />
</feature>

</widget>
